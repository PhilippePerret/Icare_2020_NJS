{
  "name": "icare_2020_njs",
  "version": "0.0.3",
  "description": "Site de l'Atelier Icare en version node.js",
  "main": "app.js",
  "scripts": {
    "--test": "MODE_TEST=true node ./app",
    "start-dev": "nodemon app.js",
    "start": "node .",
    "test": "nodemon app.js &",
    "posttest": "npm run open-google-for-fronttests",
    "open-google-for-fronttests": "open -a 'Google Chrome' 'http://localhost:3000/ftt'",
    "google": "open -a 'Google Chrome' 'http://localhost:3000'",
    "firefox": "open -a Firefox 'http://localhost:3000'",
    "watch-sass": "node-sass -r -w --output=lib/css --output-style=compressed lib/css/_sass/",
    "watch-sass-all": "node-sass -w --recursive --incluce-path=lib/css/_sass_all/ lib/css/index.sass lib/css/all.css --output-style=compressed"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "uploads/*",
      "tmp/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhilippePerret/Icare_2020_NJS.git"
  },
  "author": "Philippe Perret <philippe.perret@yahoo.fr>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PhilippePerret/Icare_2020_NJS/issues"
  },
  "homepage": "https://github.com/PhilippePerret/Icare_2020_NJS#readme",
  "dependencies": {
    "ph-math": "^1.0.0",
    "ph-array": "^1.0.0",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "express-zip": "^2.0.1",
    "multer": "^1.4.1",
    "mysql-easier": "^2.1.0",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "pug": "^2.0.4",
    "smtp-client": "^0.3.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "browserify-fs": "^1.0.0",
    "fs": "0.0.1-security",
    "mocha": "^6.1.4",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  }
}
